{
  "name": "alexanderminin/telegram-bot-sdk-long-polling",
  "description": "The Unofficial Telegram Bot API PHP SDK (fork irazasyed/telegram-bot-sdk v2.0)",
  "keywords": ["telegram", "telegram bot", "telegram bot api", "telegram sdk", "telegram php", "laravel telegram", "laravel", "long polling"],
  "version": "1.0.4",
  "type": "library",
  "authors": [
    {
      "name": "Alexander Minin",
      "email": "a.s.minin@me.com",
      "homepage": "https://github.com/alexanderminin/telegram-bot-sdk-long-polling"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "guzzlehttp/guzzle": "~6.0",
    "illuminate/support": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "Telegram\\Bot\\": "src/"
    }
  },
  "suggest": {
    "illuminate/container": "Hold dependencies to be injected in commands constructors"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
